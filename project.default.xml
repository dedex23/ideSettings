        <property name="js.eslint.nodeInterpreter" value="$USER_HOME$/.nvm/versions/node/v4.5.0/bin/node" />
        <property name="last_opened_file_path" value="$USER_HOME$/phpunit.phar" />
        <property name="node.js.path.for.package.eslint" value="project" />
