<application>
  <component name="ProjectManager">
    <defaultProject>
      <component name="ComposerSettings" synchronizationState="SYNCHRONIZE" />
      <component name="Encoding">
        <file url="PROJECT" charset="UTF-8" />
      </component>
      <component name="InspectionProjectProfileManager">
        <profile version="1.0">
          <option name="myName" value="Project Default" />
          <inspection_tool class="Eslint" enabled="true" level="ERROR" enabled_by_default="true" />
        </profile>
        <version value="1.0" />
      </component>
      <component name="JavaScriptSettings">
        <option name="languageLevel" value="ES6" />
      </component>
      <component name="MessDetector">
        <phpmd_settings>
          <MessDetectorConfiguration tool_path="$USER_HOME$/phpmd.phar" />
        </phpmd_settings>
      </component>
      <component name="PhpCodeSniffer">
        <phpcs_settings>
          <PhpCSConfiguration standards="MySource;PEAR;PSR1;PSR2;Squiz;Zend" tool_path="/usr/bin/phpcs" />
        </phpcs_settings>
      </component>
      <component name="PhpDebugGeneral" xdebug_debug_port="9005" />
      <component name="PhpProjectSharedConfiguration" php_language_level="7.2" />
      <component name="PhpTestFrameworkVersionCache">
        <tools_cache>
          <tool tool_name="PHPUnit">
            <cache>
              <versions>
                <info id="Local" version="7.1.5" />
              </versions>
            </cache>
          </tool>
        </tools_cache>
      </component>
      <component name="PhpWorkspaceProjectConfiguration" backward_compatibility_performed="true" interpreter_name="PHP 7.2" />
      <component name="ProjectInspectionProfilesVisibleTreeState">
        <entry key="Project Default">
          <profile-state>
            <expanded-state>
              <State>
                <id />
